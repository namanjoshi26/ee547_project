<!DOCTYPE html>
<html>
<head>
  <title>User Details</title>
  <style>
    table {
      border-collapse: collapse;
      width: 100%;
    }

    th, td {
      padding: 8px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }
  </style>
</head>
<body>
  <h1>User Details</h1>
  <table>
    <tr>
      <th>Name</th>
      <th>Email</th>

    </tr>
    
    <% users.forEach((user) => { %>
      <tr>
        <td><a href="/specdetails/<%= user._id %>"><%= user.name %></a></td>
        <td><%= user.email %></td>        
      </tr>
    <% }); %>
  </table>

  <form method="POST" action="/logout">
    <button type="submit">Logout</button>
  </form>
</body>
</html>

</html>
